{% extends "MaxSymForum::simpleLayout.html.twig" %}

{% block title "Admin" %}

{% block content %}
<p><a class="symfony-button-green" href="{{ path('_admin') }}">Admin</a></p>
<p>Name : {{ name }}</p>
<p>Software : {{ software }}</p>
<p>Path : {{ path }}</p>
<p>IP : {{ ip }}</p>
<p>Port : {{ port }}</p>
<p>Protocol : {{ protocol }}</p>
<p>Agent : {{ agent }}</p>
<p>Signature : {{ signature }}</p>

{% endblock %}