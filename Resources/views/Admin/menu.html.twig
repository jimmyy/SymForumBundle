{% extends "MaxSymForum::simpleLayout.html.twig" %}

{% block title "Admin" %}

{% block content %}

  <p><a class="symfony-button-green" href="{{ path('_admin_new_menu') }}">Menu</a> Total : {{ count }} menus</p>
  {% for item in entity %}
        <p>{{ item.id }} <a href="{{ path('_admin_delete', {'id': item.id, 'entity': 'Max\\SymForumBundle\\Entity\\Menu' }) }}">Delete</a></p>
  {% endfor %}

{% endblock %}